{
    "name": "ap-ionic-calendar",
    "version": "0.0.1",
    "description": "Ionic calendar component",
    "keywords": [
        "Ionic2",
        "calendar"
    ],
    "author": {
        "name": "lbertenasco"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lbertenasco/ap-ionic-calendar.git"
    },
    "scripts": {
        "build": "rm -rf dist && tsc",
        "build-prod": "rm -rf aot && node_modules/.bin/ngc -p tsconfig-aot.json && node_modules/.bin/ngc src && mv src/*ngfactory.ts aot && rm -r aot/waste && rm -r src/*.ngsummary.json",
        "copy_static_files": "cp -r package.json README.md LICENSE tsconfig.json typings.json typings dist/",
        "copy_static_files_prod": "cp -r package.json README.md LICENSE tsconfig.json typings.json typings aot/",
        "dev": "tsc --watch",
        "postinstall": "typings install"
    },
    "main": "./index.js",
    "dependencies": {},
    "devDependencies": {
        "tslint-ionic-rules": "0.0.5",
        "typescript": "2.0.3",
        "typings": "1.3.1",
        "@angular/common": "2.4.8",
        "@angular/compiler": "2.4.8",
        "@angular/compiler-cli": "2.4.8",
        "@angular/core": "2.4.8",
        "@angular/forms": "2.4.8",
        "@angular/http": "2.4.8",
        "@angular/platform-browser": "2.4.8",
        "@angular/platform-browser-dynamic": "2.4.8",
        "@angular/platform-server": "2.4.8",
        "ionic-angular": "2.3.0",
        "rxjs": "5.0.1",
        "zone.js": "0.7.2",
        "intl": "1.2.5"
    }
}
