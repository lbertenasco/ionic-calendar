{
    "name": "ap-ionic-calendar",
    "version": "0.0.10",
    "description": "Ionic calendar component",
    "keywords": [
        "Ionic2",
        "calendar"
    ],
    "author": {
        "name": "lbertenasco"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lbertenasco/ap-ionic-calendar.git"
    },
    "scripts": {
        "build": "rm -rf dist && tsc",
        "build-prod": "rm -rf aot && node_modules/.bin/ngc -p tsconfig-aot.json && node_modules/.bin/ngc src && rm -rf aot/waste",
        "copy_static_files": "cp -r package.json README.md LICENSE tsconfig.json typings.json typings dist/",
        "copy_static_files_prod": "cp -r package.json README.md LICENSE tsconfig.json typings.json typings aot/",
        "dev": "tsc --watch",
        "postinstall": "typings install"
    },
    "main": "./index.js",
    "devDependencies": {
        "@angular/common": "7.2.4",
        "@angular/compiler": "7.2.4",
        "@angular/compiler-cli": "7.2.4",
        "@angular/core": "7.2.4",
        "@angular/forms": "7.2.4",
        "@angular/http": "7.2.4",
        "@angular/platform-browser": "7.2.4",
        "@angular/platform-browser-dynamic": "7.2.4",
        "@angular/platform-server": "7.2.4",
        "@angular/router": "7.2.8",
        "@ionic/angular": "4.0.1",
        "@ionic/angular-toolkit": "1.3.0",
        "@types/node": "11.9.3",
        "intl": "1.2.5",
        "rxjs": "6.3.3",
        "typescript": "3.2.2",
        "typings": "2.1.1",
        "zone.js": "0.8.29"
    }
}
